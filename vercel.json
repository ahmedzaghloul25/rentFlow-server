{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts"
    }
  ],
  "crons": [
    {
      "path": "/cron/terminate-contracts",
      "schedule": "0 2 1,15 * *"
    },
    {
      "path": "/cron/create-payments",
      "schedule": "0 6 * * *"
    }
  ]
}
